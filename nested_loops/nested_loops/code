#include <iostream>

void multiplicationTable(int size)
{
    for(int i = 1; i <= size; ++i)
    {
        for(int j = 1; j <= size; ++j)
            std::cout << i << " * " << j << " = " << i * j << std::endl;

        std::cout << std::endl;
    }
}

int main()
{
    multiplicationTable(5);
    return 0;
}